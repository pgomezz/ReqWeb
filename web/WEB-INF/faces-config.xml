<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/MyBundle</base-name>
            <var>myBundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/project</base-name>
            <var>project</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/bundle_nfrType</base-name>
            <var>bundle_nfrType</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/bundle_requirement</base-name>
            <var>bundle_requirement</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/crudLabels</base-name>
            <var>crudLabels</var>
        </resource-bundle>
    </application>
</faces-config>
